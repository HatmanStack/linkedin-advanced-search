# Server Configuration
PORT=3001
NODE_ENV=development

# CORS Configuration
FRONTEND_URLS=http://localhost:3000,http://localhost:5173

AWS_REGION=your_aws_region

API_GATEWAY_BASE_URL=your_api_gateway_base

# S3 Bucket for storing screenshots (private bucket)
S3_SCREENSHOT_BUCKET_NAME=your_bucket

# CloudFront Distribution Domain for serving screenshots securely
CLOUDFRONT_DOMAIN=your_domain

# CloudFront Distribution ID (for cache invalidation if needed)
CLOUDFRONT_DISTRIBUTION_ID=your_id

# Origin Access Control ID (for reference)
ORIGIN_ACCESS_CONTROL_ID=your_oac

# LinkedIn Search Configuration
RECENCY_HOURS=6
RECENCY_DAYS=5
RECENCY_WEEKS=3
HISTORY_TO_CHECK=4
THRESHOLD=8
PAGE_NUMBER_START=1
PAGE_NUMBER_END=100

# Google Generative AI
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Puppeteer Configuration
HEADLESS=false
SLOW_MO=50
VIEWPORT_WIDTH=1200
VIEWPORT_HEIGHT=1200

# Timeout Settings
DEFAULT_TIMEOUT=30000
NAVIGATION_TIMEOUT=30000
LOGIN_SECURITY_TIMEOUT=30000

# File Paths
SCREENSHOTS_DIR=./screenshots
LINKS_FILE=./data/possible-links.json
GOOD_CONNECTIONS_FILE=./data/good-connections-links.json